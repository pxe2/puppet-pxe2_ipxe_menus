:older_release
set older_release true
set oraclelinux_mirror old-releases.oraclelinux.com
echo Setting mirror to ${oraclelinux_mirror}
clear oraclelinux_version
echo -n Please set enter code name of release: ${} && read oraclelinux_version
set dir ${oraclelinux_base_dir}/dists/${oraclelinux_version}/main/installer-${arch_a}/current/images/netboot/
goto oraclelinux_boot_type

:mirrorcfg
set mirrorcfg mirror/suite=${oraclelinux_version}
set dir ${oraclelinux_base_dir}/dists/${oraclelinux_version}/main/installer-${arch_a}/current/images/netboot/

:oraclelinux_boot_type
menu ${os} [${oraclelinux_version}] Installer
item --gap Install types
item install ${space} Install
item rescue ${space} Rescue Mode
item expert ${space} Expert Install
item preseed ${space} Specify preseed url...
choose --default ${type} type || goto oraclelinux
echo ${cls}
goto oraclelinux_${type}

:oraclelinux_rescue
set install_params rescue/enable=true
goto oraclelinux_boot

:oraclelinux_expert
set install_params priority=low
goto oraclelinux_boot

:oraclelinux_preseed
echo -n Specify preseed URL for ${os} ${oraclelinux_version}: && read preseedurl
set install_params auto=true priority=critical preseed/url=${preseedurl}
goto oraclelinux_boot

:oraclelinux_install
:oraclelinux_boot
set dir ${dir}${menu}-installer/${arch_a}
imgfree
kernel http://${oraclelinux_mirror}/${dir}/linux ${install_params} ${netcfg} ${mirrorcfg} ${console} -- quiet ${params} initrd=initrd.gz
initrd http://${oraclelinux_mirror}/${dir}/initrd.gz
echo
echo MD5sums:
md5sum linux initrd.gz
iseq ${img_sigs_enabled} true && iseq ${older_release} true && goto skip_sigs ||
iseq ${img_sigs_enabled} true && goto verify_sigs || goto skip_sigs
:verify_sigs
echo
echo Checking signatures...
imgverify linux ${sigs}${dir}/linux.sig || goto error
imgverify initrd.gz ${sigs}${dir}/initrd.gz.sig || goto error
echo Signatures verified!
echo
:skip_sigs
boot

:oraclelinux_exit
clear menu
exit 0
