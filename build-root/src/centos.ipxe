:older_release
set older_release true
set centos_mirror old-releases.centos.com
echo Setting mirror to ${centos_mirror}
clear centos_version
echo -n Please set enter code name of release: ${} && read centos_version
set dir ${centos_base_dir}/dists/${centos_version}/main/installer-${arch_a}/current/images/netboot/
goto centos_boot_type

:mirrorcfg
set mirrorcfg mirror/suite=${centos_version}
set dir ${centos_base_dir}/dists/${centos_version}/main/installer-${arch_a}/current/images/netboot/

:centos_boot_type
menu ${os} [${centos_version}] Installer
item --gap Install types
item install ${space} Install
item rescue ${space} Rescue Mode
item expert ${space} Expert Install
item preseed ${space} Specify preseed url...
choose --default ${type} type || goto centos
echo ${cls}
goto centos_${type}

:centos_rescue
set install_params rescue/enable=true
goto centos_boot

:centos_expert
set install_params priority=low
goto centos_boot

:centos_preseed
echo -n Specify preseed URL for ${os} ${centos_version}: && read preseedurl
set install_params auto=true priority=critical preseed/url=${preseedurl}
goto centos_boot

:centos_install
:centos_boot
set dir ${dir}${menu}-installer/${arch_a}
imgfree
kernel http://${centos_mirror}/${dir}/linux ${install_params} ${netcfg} ${mirrorcfg} ${console} -- quiet ${params} initrd=initrd.gz
initrd http://${centos_mirror}/${dir}/initrd.gz
echo
echo MD5sums:
md5sum linux initrd.gz
iseq ${img_sigs_enabled} true && iseq ${older_release} true && goto skip_sigs ||
iseq ${img_sigs_enabled} true && goto verify_sigs || goto skip_sigs
:verify_sigs
echo
echo Checking signatures...
imgverify linux ${sigs}${dir}/linux.sig || goto error
imgverify initrd.gz ${sigs}${dir}/initrd.gz.sig || goto error
echo Signatures verified!
echo
:skip_sigs
boot

:centos_exit
clear menu
exit 0
